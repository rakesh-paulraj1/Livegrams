{
  "name": "Livegrams",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "start:web": "cd apps/web && pnpm run start",
    "start:ws": "cd apps/websocket && pnpm run start",
    "build:ws": "cd apps/websocket && pnpm run build",
    "build:web": "cd apps/web && pnpm run build",
    "db:generate": "cd packages/db && npx prisma generate  && cd  ../..",
    "db:push": "cd packages/db && pnpm push",
    "db:migrate": "cd packages/db && pnpm migrate"
  },
  "devDependencies": {
    "@types/pg": "^8.15.5",
    "drizzle-kit": "^0.31.4",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "turbo": "^2.6.3",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@langchain/pinecone": "^1.0.0",
    "dotenv": "^17.2.2",
    "drizzle-orm": "^0.44.5",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "pnpm": "^10.18.3"
  }
}
